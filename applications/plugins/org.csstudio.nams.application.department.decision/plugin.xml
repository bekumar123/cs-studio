<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         id="org.csstudio.nams.application.department.decision.application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.csstudio.nams.application.department.decision.DecisionDepartmentActivator">
         </run>
      </application>
   </extension>
   <extension
         point="org.csstudio.remote.managementCommands">
      <command
            class="org.csstudio.nams.application.department.decision.management.Stop"
            id="org.csstudio.nams.application.department.decision.management.Stop"
            label="Stop">
         <stringParameter
               id="Password"
               label="Password">
         </stringParameter>
      </command>
      <command
            class="org.csstudio.nams.application.department.decision.management.Restart"
            id="org.csstudio.nams.application.department.decision.management.Restart"
            label="Restart">
         <stringParameter
               id="Password"
               label="Password">
         </stringParameter>
      </command>
      <command
            class="org.csstudio.nams.application.department.decision.management.InfoCmd"
            id="org.csstudio.nams.application.department.decision.management.InfoCmd"
            label="Info">
      </command>
   </extension>
   <extension
         id="AmsDepartmentDecisionProduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.csstudio.nams.application.department.decision.application"
            name="AmsDepartmentDecisionProduct">
         <property
               name="appName"
               value="AmsDepartmentDecisionProduct">
         </property>
      </product>
   </extension>

</plugin>
