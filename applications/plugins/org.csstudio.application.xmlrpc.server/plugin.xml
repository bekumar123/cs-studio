<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
>
   <extension
         point="org.eclipse.equinox.preferences.preferences">
      <initializer
            class="org.csstudio.application.xmlrpc.server.internal.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         id="ServerApplication"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.csstudio.application.xmlrpc.server.ServerApplication">
         </run>
      </application>
   </extension>
   <extension
         point="org.csstudio.remote.managementCommands">
      <command
            class="org.csstudio.application.xmlrpc.server.management.Stop"
            id="org.csstudio.application.xmlrpc.server..Stop"
            label="Stop">
      </command>
      <command
            class="org.csstudio.application.xmlrpc.server.management.GetInfo"
            id="org.csstudio.application.xmlrpc.server.GetInfo"
            label="Get Info">
      </command>
   </extension>
   <extension
         id="XmlRpcServerProduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.csstudio.application.xmlrpc.server.ServerApplication"
            name="XmlRpcServer">
         <property
               name="appName"
               value="XmlRpcServer">
         </property>
      </product>
   </extension>
</plugin>
