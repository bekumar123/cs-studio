<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="Tine2Jms"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.csstudio.tine2jms.TineToJmsApplication">
         </run>
      </application>
   </extension>
   <extension
         point="org.csstudio.remote.managementCommands">
      <command
            class="org.csstudio.tine2jms.management.StopCmd"
            id="org.csstudio.tine2jms.management.Stop"
            label="Stop">
      </command>
      <command
            class="org.csstudio.tine2jms.management.RestartCmd"
            id="org.csstudio.tine2jms.management.Restart"
            label="Restart">
      </command>
      <command
            class="org.csstudio.tine2jms.management.InfoCmd"
            id="org.csstudio.tine2jms.management.Info"
            label="Info">
      </command>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.csstudio.tine2jms.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         id="Tine2JmsProduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.csstudio.tine2jms.Tine2Jms"
            name="Tine2JmsProduct">
         <property
               name="appName"
               value="Tine2JmsProduct">
         </property>
      </product>
   </extension>

</plugin>
